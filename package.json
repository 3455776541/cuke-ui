{
  "name": "cuke-ui",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/cuke-ui/cuke-ui.git",
  "author": "Jinke.Li <jkli@thoughtworks.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "yarn dev",
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production && yarn run clean && webpack --env.mode=production --mode production --progress --config webpack.config.js",
    "dev": "start-storybook -p 8080 -c .storybook",
    "upgrade": "yarn update",
    "precommit": "lint-staged",
    "stylelint:fix": "stylelint 'components/**/*.less' --syntax less --fix",
    "eslint:fix": "eslint_d --fix components",
    "lint": "prettier --write \"components/**/*.js\" && yarn eslint:fix && yarn stylelint:fix",
    "test": "jest --config jest.config.js",
    "coverage": "yarn test -- --coverage",
    "ci": "yarn coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "doc": "build-storybook -c .storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "pre-commit": "lint",
  "lint-staged": {
    "components/**/*.js": [
      "prettier --write",
      "eslint_d --fix",
      "git add"
    ]
  },
  "keywords": [
    "cuke-ui",
    "study",
    "react",
    "components",
    "ui"
  ],
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.44",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.44",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.44",
    "@babel/plugin-transform-object-assign": "7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-react": "7.0.0-beta.44",
    "@babel/preset-stage-0": "7.0.0-beta.44",
    "@babel/preset-stage-1": "7.0.0-beta.44",
    "@babel/runtime": "7.0.0-beta.44",
    "@storybook/react": "^3.4.10",
    "@storybook/storybook-deployer": "^2.3.0",
    "autoprefixer": "^9.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint_d": "^7.1.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "jest": "^22.4.3",
    "jest-environment-jsdom": "^22.4.3",
    "jsdom": "^11.8.0",
    "less": "^2.7.2",
    "lint-staged": "^7.0.4",
    "open-browser-webpack-plugin": "0.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss": "^7.0.2",
    "postcss-cli": "^6.0.0",
    "power-assert": "^1.6.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.0",
    "sinon": "^5.0.7",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
