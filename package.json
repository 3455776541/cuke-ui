{
  "name": "cuke-ui",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/cuke-ui/cuke-ui.git",
  "author": "Jinke.Li <jkli@thoughtworks.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "yarn dev",
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production && yarn run clean && webpack --env.mode=production --mode production --progress --config webpack.config.js",
    "dev": "start-storybook -p 8080 -c .storybook",
    "upgrade": "yarn update",
    "precommit": "lint-staged",
    "stylelint:fix": "stylelint 'components/**/*.less' --syntax less --fix",
    "eslint:fix": "eslint ./components --fix",
    "lint": "prettier --write \"components/**/*.js\" && yarn eslint:fix && yarn stylelint:fix",
    "test": "jest --config jest.config.js",
    "coverage": "yarn test -- --coverage",
    "ci": "yarn coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && yarn build:doc && yarn pub:doc",
    "build:doc": "build-storybook -c .storybook -o .out",
    "pub:doc": "storybook-to-ghpages --existing-output-dir=.out"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "components/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "prettier --write",
      "stylelint --syntax less --fix",
      "git add"
    ],
    "tests": [
      "jest --config jest.config.js"
    ]
  },
  "storybook-deployer": {
    "gitUsername": "cuke-ui",
    "gitEmail": "1359518268@qq.com",
    "commitMessage": "docs: deploy docs"
  },
  "keywords": [
    "cuke-ui",
    "study",
    "react",
    "components",
    "ui"
  ],
  "devDependencies": {
    "@storybook/addon-info": "^3.4.10",
    "@storybook/addon-options": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@storybook/storybook-deployer": "^2.3.0",
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "latest",
    "coveralls": "^3.0.1",
    "css-loader": "~0.23.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "imagemin-webpack-plugin": "^1.4.4",
    "jest": "^22.4.3",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "lint-staged": "^7.0.4",
    "normalize.css": "^8.0.0",
    "open-browser-webpack-plugin": "0.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss": "^6.0.12",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^1.2.2",
    "power-assert": "^1.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-loader": "^2.4.0",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.0",
    "sinon": "^5.0.7",
    "style-loader": "~0.13.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^0.5.8"
  },
  "dependencies": {
    "react-icons": "^3.0.5",
    "react-jinke-music-player": "^4.0.3"
  }
}
